{
  "name": "noem/tiny-proxy",
  "description": "Modular service container",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "squizlabs/php_codesniffer": "^3.6",
    "noem/composer-file-embed": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Noem\\TinyProxy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Noem\\TinyProxy\\Tests\\": "tests/src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
